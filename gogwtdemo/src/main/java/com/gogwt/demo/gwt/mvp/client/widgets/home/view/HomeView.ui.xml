<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
     xmlns:g="urn:import:com.google.gwt.user.client.ui">
     
     <ui:style field='resStyle' src="../../gogwt_mvp.css"/>
     
     <g:HorizontalPanel>
           
        <!-- Left res search form -->
        <g:VerticalPanel styleName="{resStyle.homeViewLeft}">
           <g:HTMLPanel>
           
              <g:Label styleName="{resStyle.text12blue}" ui:field="pageDescLabel"/>
              <g:Label styleName="{resStyle.text12blue}" ui:field="welcomeLabel"/>
              
              
               
              <table> 
                  <tr class='{resStyle.item}'>
                     <td align="right">  Greeting: </td>  <td> <g:TextBox ui:field="detail"/> </td>
                  </tr>  
                  <tr class='{resStyle.item}'>
                     <td align="right">  Title: </td>  <td> <g:ListBox ui:field="title"/> </td>
                  </tr>  
                  <tr>         
                     <td align="right">  First Name: </td>  <td> <g:TextBox ui:field="firstName"/> </td>                     
                  </tr>
                  <tr>         
                     <td align="right">  Last Name: </td>  <td> <g:TextBox ui:field="lastName"/> </td>                     
                  </tr>                 
                   <tr>         
                     <td align="right" colspan="2">  <g:Button  ui:field="toDetailBtn"/> </td>                     
                  </tr>                
               </table>               
            </g:HTMLPanel>  
        </g:VerticalPanel>
        
    
     </g:HorizontalPanel>
</ui:UiBinder>