<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
     xmlns:g="urn:import:com.google.gwt.user.client.ui">
     
     <ui:style field='resStyle' src="../../../../../gogwt_mvp.css"/>
  
     <g:HTMLPanel>
         <br/>       
          <div class='{resStyle.item}'> <B><span ui:field='name'/></B> </div>
          <div class='{resStyle.item}'>  <span ui:field='address'/>  </div>
          <br/>
          <div class='{resStyle.item}'><b><span ui:field='amenityLabel'/>:</b> <span ui:field='amenities'/>   </div>
          <div class='{resStyle.item}'><span ui:field='description'/>   </div>
          <div class='{resStyle.item}'><span ui:field='latLng'/>   </div>
          <div class='{resStyle.item}'><span ui:field='checkInTime'/>   </div>
          <div class='{resStyle.item}'><span ui:field='checkOutTime'/>   </div>
          <div class='{resStyle.item}'><span ui:field='numberOfRooms'/>   </div>     
     </g:HTMLPanel>
</ui:UiBinder>