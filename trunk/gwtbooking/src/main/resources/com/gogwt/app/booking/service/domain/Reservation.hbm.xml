<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.gogwt.app.booking.dto.dataObjects.request">

    <class name="ReservationBean" table="reservation">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        
        <property name="propertyId" column="property_id"/>
        <property name="firstName" column="first_name"/>
        <property name="lastName" column="last_name"/>
        <property name="address"/>
        <property name="city"/>
        <property name="state"/>
        <property name="zip"/>
        <property name="email"/>
        <property name="countryId" column="country_id"/>
        <property name="languageId" column="language_id"/>
        <property name="createDate" column="create_date"/>
        
    </class>
</hibernate-mapping>